#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+serve@2.16.8_typescript@5.8.3/node_modules/@remix-run/serve/dist/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+serve@2.16.8_typescript@5.8.3/node_modules/@remix-run/serve/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+serve@2.16.8_typescript@5.8.3/node_modules/@remix-run/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+serve@2.16.8_typescript@5.8.3/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+serve@2.16.8_typescript@5.8.3/node_modules/@remix-run/serve/dist/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+serve@2.16.8_typescript@5.8.3/node_modules/@remix-run/serve/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+serve@2.16.8_typescript@5.8.3/node_modules/@remix-run/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+serve@2.16.8_typescript@5.8.3/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
