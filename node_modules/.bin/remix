#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_react-dom@18.3.1_react@18.3.1__react@18.3_5b2edb8f1e61cf418ddc62358c5e329f/node_modules/@remix-run/dev/dist/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_react-dom@18.3.1_react@18.3.1__react@18.3_5b2edb8f1e61cf418ddc62358c5e329f/node_modules/@remix-run/dev/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_react-dom@18.3.1_react@18.3.1__react@18.3_5b2edb8f1e61cf418ddc62358c5e329f/node_modules/@remix-run/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_react-dom@18.3.1_react@18.3.1__react@18.3_5b2edb8f1e61cf418ddc62358c5e329f/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_react-dom@18.3.1_react@18.3.1__react@18.3_5b2edb8f1e61cf418ddc62358c5e329f/node_modules/@remix-run/dev/dist/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_react-dom@18.3.1_react@18.3.1__react@18.3_5b2edb8f1e61cf418ddc62358c5e329f/node_modules/@remix-run/dev/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_react-dom@18.3.1_react@18.3.1__react@18.3_5b2edb8f1e61cf418ddc62358c5e329f/node_modules/@remix-run/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_react-dom@18.3.1_react@18.3.1__react@18.3_5b2edb8f1e61cf418ddc62358c5e329f/node_modules:/home/hamzah/Desktop/vibrant_matching_puzzle_game_with_react_and_remix_umf2kg/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@remix-run/dev/dist/cli.js" "$@"
else
  exec node  "$basedir/../@remix-run/dev/dist/cli.js" "$@"
fi
